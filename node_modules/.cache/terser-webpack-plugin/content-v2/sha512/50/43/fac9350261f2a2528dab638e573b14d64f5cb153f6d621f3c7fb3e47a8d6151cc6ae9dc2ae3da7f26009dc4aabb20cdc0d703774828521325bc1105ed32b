Favs = [...favorites]; \n    newFavs[index] = value;\n    if (currentIndex === -1) {\n      newChecked.push(value);\n    } else {\n      newChecked.splice(currentIndex, 1);\n    }\n\n    setChecked(newChecked)\n    setFavorites(newFavs);\n    \n  };\n    return(\n        <ListItem key={props.key} value ={props.value} id={props.id}>\n        <ListItemAvatar>\n          <StyledImage\n            alt={`This is the cover for the Song ${props.name}`}\n            src={props.src}\n          />\n        </ListItemAvatar>\n        <Box display=\"block\" maxWidth=\"400px\">\n            <StyledTypography variant=\"h5\"> {props.song}</StyledTypography>\n            <StyledTypography variant=\"body1\"> {props.artist}</StyledTypography>\n        </Box>\n        <ListItemSecondaryAction>\n          <Box display=\"flex\">\n            <a href={props.href}>\n              <Typography color=\"textSecondary\" variant=\"body1\">\n                {`Buy for ${props.price}`}\n              </Typography>\n            </a>\n            <Checkbox\n                edge=\"end\"\n                onChange={handleToggle(props.name, props.index)}\n                checked={checked.indexOf(props.name) !== -1}\n                inputProps={{ 'aria-labelledby': props.id }}\n              />\n          </Box>\n        </ListItemSecondaryAction>\n      </ListItem>\n    )\n\n}","import React from 'react';\nimport TextField from '@material-ui/core/TextField';\nimport Autocomplete from '@material-ui/lab/Autocomplete';\n\nexport function GroupedSearchInput(props) {\n  const data = props.data;\n  const options = data.map((option) => {\n    const firstLetter = option[\"im:name\"].label[0].toUpperCase();\n    return {\n      firstLetter: /[0-9]/.test(firstLetter) ? '0-9' : firstLetter,\n      ...option,\n    };\n  });\n\n  return (\n    <Autocomplete\n      data-testid=\"input\"\n      id=\"search-input\"\n      options={options.sort((a, b) => -b.firstLetter.localeCompare(a.firstLetter))}\n      groupBy={(opt